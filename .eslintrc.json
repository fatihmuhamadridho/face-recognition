{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "tsx": true
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-sort-props": [1, { "ignoreCase": true }]
  },
  "extends": ["eslint:recommended", "next", "prettier"]
}
